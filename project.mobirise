al\" data-target=\"#{{idGallery}}-modal\">\n                    <a href=\"https://godotengine.org/\" target=\"_blank\"><img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/godot-1-506x205.png\" alt data-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\"></a>\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-2\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\">Godot Engine</h6>\n            </div><div class=\"col-12 col-md-6 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\">\n                    <a href=\"https://www.canva.com/\" target=\"_blank\"><img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/canva-1-211x106.png\" alt data-target=\"#lb-{{idGallery}}\" data-slide-to=\"4\"></a>\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-2\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\">Canva</h6>\n            </div><div class=\"col-12 col-md-6 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\">\n                    <a href=\"https://www.figma.com/\" target=\"_blank\"><img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/figma-1-720x720.png\" alt data-target=\"#lb-{{idGallery}}\" data-slide-to=\"4\"></a>\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </